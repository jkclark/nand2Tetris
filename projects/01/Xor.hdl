/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=notA);
    Not(in=b, out=notB);
    Nand(a=notA, b=notB, out=notNandOut);
    Nand(a=a, b=b, out=nandOut);
    And(a=notNandOut, b=nandOut, out=out);
}
